,[                   // Start loop
  ->+>+<<[->>>+<<<]  // Read the first digit into cell 1 and the second digit into cell 2
  [
    -                // Subtract 1 from cell 2 to convert ASCII digit to numerical value
    [->+<]           // Add the value in cell 1 to the value in cell 0
    >>+              // Move to cell 2 and increment its value
  ]
  <<[->>+<<]         // Move the accumulated value from cell 0 to cell 1
  >>>[->+<]          // Move the value from cell 1 to cell 0
  <<.                // Print the result
  ,                 // Read the next digit into cell 2
  +                 // Increment cell 1 to carry over the previous result
]